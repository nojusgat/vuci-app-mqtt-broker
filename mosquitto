config mqtt 'mqtt'
    option enabled 0
    option local_port 1883
    option enable_ra 0
    option use_tls_ssl 0
    option tls_type 'cert'
    option ca_file ''
    option cert_file ''
    option key_file ''
    option tls_version 'all'
    option psk ''
    option identity ''
    option acl_file_path ''
    option password_file ''
    option persistence 0
    option anonymous_access 1
